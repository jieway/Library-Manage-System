<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.weijiew.lms.mapper.ReaderMapper">
    <select id="queryReaderList" resultType="Reader">
        select * from Reader
    </select>

    <select id="queryReaderByRNO" resultType="Reader">
        select * from Reader where RNO = #{RNO}
    </select>

</mapper>
